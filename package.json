{
  "name": "agentic-design-patterns",
  "version": "1.1.0",
  "description": "Interactive visualization of 20 agentic design patterns",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run postbuild",
    "postbuild": "node -e \"require('fs').renameSync('dist/index-vite.html', 'dist/index.html')\"",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.{js,html,css}\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^9.3.3",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ai-Whisperers/agentic-schemas.git"
  },
  "license": "MIT"
}
